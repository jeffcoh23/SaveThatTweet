{
  "expo": {
    "name": "SaveThatTweet",
    "slug": "SaveThatTweet",
    "scheme": "savethattweet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/save-that-tweet-logo.png",
    "splash": {
      "image": "./assets/save-that-tweet-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#81bffd"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "associatedDomains": [
        "applinks:savethattweet.com",
        "applinks:www.savethattweet.com"
      ],
      "supportsTablet": true,
      "bundleIdentifier": "com.jeffcoh23.SaveThatTweet"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
